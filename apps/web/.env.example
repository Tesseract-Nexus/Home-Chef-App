# Environment Configuration
# Copy this file to .env.development for local development

# Mock Mode - Set to 'true' to use mock data, 'false' for real API
VITE_MOCK_MODE=true

# API Configuration
VITE_API_URL=http://localhost:8080/api/v1
VITE_WS_URL=ws://localhost:8080/ws

# OAuth Providers
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_FACEBOOK_APP_ID=your-facebook-app-id
VITE_APPLE_CLIENT_ID=your-apple-client-id

# Payment
VITE_STRIPE_PUBLIC_KEY=pk_test_xxxxx

# Maps
VITE_GOOGLE_MAPS_KEY=your-google-maps-key

# Feature Flags
VITE_ENABLE_CATERING=true
VITE_ENABLE_SOCIAL_FEED=true

# App Info
VITE_APP_NAME=HomeChef
VITE_APP_VERSION=1.0.0
